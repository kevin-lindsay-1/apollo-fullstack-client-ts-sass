{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "lint": "tsc --noEmit && tslint -c tslint.json -p tsconfig.json",
    "test": "npm run lint && react-scripts test",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "docker:build": "env-cmd --fallback .env.local cross-var docker build -t fullstack-client --build-arg \"REACT_APP_API_URI=$REACT_APP_API_URI\" .",
    "docker:run": "env-cmd --fallback .env.local cross-var docker run --rm -it -e APP_LISTEN_PORT=$APP_LISTEN_PORT -p $APP_LISTEN_PORT:$APP_LISTEN_PORT --name fullstack-client fullstack-client"
  },
  "dependencies": {
    "serve": "^10.1.1"
  },
  "devDependencies": {
    "@reach/router": "^1.2.1",
    "@types/graphql": "^14.0.4",
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.18",
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "apollo-cache-inmemory": "alpha",
    "apollo-client": "alpha",
    "apollo-link-context": "^1.0.12",
    "apollo-link-http": "^1.5.9",
    "cross-var": "^1.1.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "jest-dom": "^3.0.0",
    "node-sass": "^4.11.0",
    "prettier-stylelint": "^0.4.2",
    "prettier-tslint": "^0.4.2",
    "react": "^16.7.0",
    "react-apollo": "alpha",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.2",
    "react-testing-library": "^5.4.2",
    "stylelint": "^9.9.0",
    "stylelint-config-idiomatic-order": "^6.2.0",
    "stylelint-config-recommended": "^2.1.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
